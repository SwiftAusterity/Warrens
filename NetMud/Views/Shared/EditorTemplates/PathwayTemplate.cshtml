@using NetMud.Data.Room

@model PathwayTemplate

<h4>Connections</h4>
<div class="form-group row">
    @Html.EditorFor(m => m.Origin, "RoomTemplateList", new { validItems = ViewData["ValidRooms"], disabled = true })
    @Html.EditorFor(m => m.Destination, "RoomTemplateList", new { validItems = ViewData["ValidRooms"], disabled = true })
</div>

<h4>Details</h4>
<div class="form-group row">
    @Html.EditorFor(m => m.Name)
    @Html.EditorFor(m => m.InclineGrade)
    @Html.EditorFor(m => m.DegreesFromNorth)
</div>

@Html.EditorFor(m => m.Model)
