@using NetMud.Data.NPC.IntelligenceControl
@model Preference
<div class="accordionWrapper">
    <h5>@Model.Context.ToString() : @Model.Quality</h5>
    <div>
        <div class="form-group row">
            <a href="#" class="removeRow col-form-label" title="Remove this" onclick="javascript: $(this).parent().parent().parent().remove(); return false;">X</a>
            @Html.EditorFor(m => m.Context)
            @Html.EditorFor(m => m.Quality)
            @Html.EditorFor(m => m.Multiplier)
        </div>
    </div>
</div>