@model HashSet<string>
@{
    var currentCount = Model.Count();
    var rowsName = ViewData.ModelMetadata.PropertyName + "Rows";
}
<div id="@rowsName" class="iterativeContainer">
    <div class="addRowContainer">
        <div>
            @Html.DescriptionFor(m => m)
            <a href="#" class="addRow btn" data-current-row="@currentCount" onclick="javascript: replacePart(this, newTagRow, @currentCount, '@rowsName'); return false;"><span class="addWord">Add Tag</span></a>
        </div>
    </div>
    @if (currentCount > 0)
    {
        @Html.EditorForMany(m => m, null, 0, "TagItem")
    }
</div>
<script type="text/javascript">
    var newTagRow = decodeHtml('@Html.EmptyEditorForMany(m => m, null, currentCount, "TagItem")');
</script>